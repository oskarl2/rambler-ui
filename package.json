{
  "name": "rambler-ui",
  "version": "3.50.0",
  "description": "Rambler UI Components based on React.js",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "karma": "NODE_ENV=test /usr/bin/env node --max-old-space-size=4096 ./node_modules/karma/bin/karma start",
    "test": "npm run karma -- --browsers ChromeC",
    "test:firefox": "npm run karma -- --browsers FirefoxC",
    "test:ci": "npm run karma -- --browsers ChromeC,FirefoxC",
    "test:watch": "npm run karma -- --browsers ChromeC --auto-watch --no-single-run",
    "prebuild": "rm -rf build",
    "build": "NODE_ENV=production babel src --out-dir build --ignore **/*.test.js && node copy-files.js",
    "start": "NODE_ENV=development webpack-dev-server -d --hot --config docs/webpack.config.js",
    "prepages": "rm -rf docs/build",
    "pages": "NODE_ENV=production webpack -p --progress --config docs/webpack.config.js && node docs/scripts/publish-site.js",
    "lint": "eslint '**/*.js' && eslint --config ./.eslintrc.typescript.json '**/*.ts'",
    "format": "prettier --write '**/*.{js,ts}' && eslint '**/*.js' --fix && eslint --config ./.eslintrc.typescript.json '**/*.ts' --fix",
    "size": "size-limit",
    "types": "tsc",
    "precommit": "lint-staged",
    "postversion": "git push --follow-tags"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.rambler.ru:rambler-ui/rambler-ui.git"
  },
  "author": "Rambler team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rambler-digital-solutions/rambler-ui/issues"
  },
  "homepage": "https://rambler-digital-solutions.github.io/rambler-ui",
  "dependencies": {
    "classnames": "^2.2.6",
    "deepmerge": "^3.2.0",
    "eventemitter3": "^3.1.0",
    "jss": "^9.8.7",
    "jss-preset-default": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "react-jss": "^8.6.1"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.15.4",
    "@mdx-js/mdx": "^0.15.4",
    "@types/jss": "^9.5.8",
    "@types/node": "^12.7.8",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.2",
    "@types/react-jss": "^8.6.4",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-istanbul": "^5.0.1",
    "babel-plugin-preval": "^1.6.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.8.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-inline-environment-variables": "^0.3.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "babel-polyfill": "^6.9.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.5.0",
    "babel-standalone": "^6.26.0",
    "color": "^1.0.3",
    "css-loader": "^3.2.0",
    "debug": "^2.2.0",
    "doctrine": "^2.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^4.2.0",
    "get-md-title": "^1.0.4",
    "gh-pages": "^0.12.0",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.11.4",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.7",
    "karma": "^4.2.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^1.1.2",
    "karma-firefox-launcher": "^1.2.0",
    "karma-jasmine": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^6.1.0",
    "minimist": "^1.2.0",
    "prettier": "^1.18.2",
    "promise-queue": "^2.2.5",
    "puppeteer": "^1.19.0",
    "raw-loader": "^3.1.0",
    "react": "^16.2.0",
    "react-codemirror": "^1.0.0",
    "react-docgen": "^2.20.1",
    "react-dom": "^16.2.0",
    "react-highlight.js": "^1.0.7",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.0.0",
    "recast": "^0.13.2",
    "remark": "^9.0.0",
    "remark-highlight.js": "^5.0.0",
    "remark-html": "^7.0.0",
    "size-limit": "^0.22.0",
    "sticky-sidebar": "^3.3.1",
    "style-loader": "^1.0.0",
    "typescript": "^3.6.3",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0"
  },
  "keywords": [
    "react",
    "ui",
    "rambler&co"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "prettier --write",
      "eslint --config ./.eslintrc.typescript.json --fix",
      "git add"
    ]
  }
}
